<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/skb_supervisor_dashboard"
    app:startDestination="@id/SKBDashboardFragment">

    <fragment
        android:id="@+id/SKBDashboardFragment"
        android:name="com.hul.skb.supervisor.ui.dashboard.SKBDashboardFragment"
        android:label="SKBDashboardFragment" >
        <action
            android:id="@+id/action_SKBDashboardFragment_to_attendencFragment"
            app:destination="@id/attendencFragment" />
        <action
            android:id="@+id/action_SKBDashboardFragment_to_supervisorVisitsFragment2"
            app:destination="@id/supervisorVisitsFragment2" />
    </fragment>
    <fragment
        android:id="@+id/attendencFragment"
        android:name="com.hul.skb.supervisor.ui.attendence.AttendencFragment"
        android:label="AttendencFragment" />
    <fragment
        android:id="@+id/supervisorVisitsFragment2"
        android:name="com.hul.skb.supervisor.ui.visits.SupervisorVisitsFragment"
        android:label="SupervisorVisitsFragment" />
</navigation>